{
  "name": "reflaction",
  "version": "0.6.0",
  "description": "React state management using the flux pattern",
  "keywords": [
    "react",
    "state",
    "flux",
    "store",
    "dispatch",
    "shared",
    "connect",
    "provider"
  ],
  "main": "build/index.js",
  "scripts": {
    "build": "babel src -d build",
    "build:clean": "rm -rf build",
    "prepublishOnly": "npm run build:clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshmanderson/reflaction.git"
  },
  "author": "Josh Manderson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshmanderson/reflaction/issues"
  },
  "homepage": "https://github.com/joshmanderson/reflaction#readme",
  "dependencies": {
    "hoist-non-react-statics": "^2.5.0",
    "prop-types": "^15.6.1"
  },
  "peerDependencies": {
    "react": "^16.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "react": "^16.2.0"
  },
  "babel": {
    "presets": [
      "react",
      "env"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  }
}
